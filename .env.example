# PostgreSQL数据库配置
# 开发环境配置
NODE_ENV=development

# 数据库连接信息
DB_HOST=localhost
DB_PORT=5432
DB_NAME=newpms2_dev
DB_USER=postgres
DB_PASSWORD=your_password_here

# 生产环境配置 (部署时修改)
# DB_HOST=your_production_host
# DB_PORT=5432
# DB_NAME=newpms2_prod
# DB_USER=your_prod_user
# DB_PASSWORD=your_prod_password

# JWT密钥 (请修改为随机字符串)
JWT_SECRET=your_jwt_secret_key_here

# 服务器端口
PORT=3000
